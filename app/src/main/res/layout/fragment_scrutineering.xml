<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mvp.view.activity.teams.TeamsActivity">
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <View
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:background="@drawable/toolbar_shadow" />

        <ScrollView 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="16dp"
            android:scrollbars="none"
            android:fillViewport="true">
            
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ai_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Accumulator Insp."
                                android:layout_marginEnd="8dp"
                                android:textColor="@color/md_grey_800"
                                android:textSize="20sp" />

                            <info.androidhive.fontawesome.FontTextView
                                android:id="@+id/ai_check"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/fa_check_solid"
                                android:textColor="@color/md_green_500"
                                android:textSize="30sp"
                                app:solid_icon="true" />

                        </LinearLayout>

                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/ai_button"
                            style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/md_green_500"
                            app:rippleColor="@color/md_green_500"
                            app:backgroundTint="@color/md_white_1000"
                            app:strokeColor="@color/md_green_500"
                            android:text="PASS"/>

                    </LinearLayout>


                    <EditText
                        android:id="@+id/ai_comments"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:inputType="text|textMultiLine"
                        android:hint="Comments"
                        android:focusable="false"
                        android:padding="8dp"
                        android:textSize="14sp"
                        android:background="@drawable/text_area_rounded"
                        android:gravity="top"/>

                </LinearLayout>


            <LinearLayout
                android:id="@+id/ei_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:orientation="horizontal">
    
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Electrical Insp."
                            android:layout_marginEnd="8dp"
                            android:textColor="@color/md_grey_800"
                            android:textSize="20sp" />

                        <info.androidhive.fontawesome.FontTextView
                            android:id="@+id/ei_check"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fa_check_solid"
                            android:textColor="@color/md_green_500"
                            android:textSize="30sp"
                            app:solid_icon="true" />

                     </LinearLayout>

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/ei_button"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/md_green_500"
                        app:rippleColor="@color/md_green_500"
                        app:backgroundTint="@color/md_white_1000"
                        app:strokeColor="@color/md_green_500"
                        android:text="PASS"/>
    
                </LinearLayout>

                <EditText
                    android:id="@+id/ei_comments"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:inputType="text|textMultiLine"
                    android:hint="Comments"
                    android:focusable="false"
                    android:padding="8dp"
                    android:textSize="14sp"
                    android:background="@drawable/text_area_rounded"
                    android:gravity="top"/>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/mi_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:orientation="horizontal">
    
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Mechanical Insp."
                            android:layout_marginEnd="8dp"
                            android:textColor="@color/md_grey_800"
                            android:textSize="20sp" />

                        <info.androidhive.fontawesome.FontTextView
                            android:id="@+id/mi_check"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fa_check_solid"
                            android:textColor="@color/md_green_500"
                            android:textSize="30sp"
                            app:solid_icon="true" />

                    </LinearLayout>

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/mi_button"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/md_green_500"
                        app:rippleColor="@color/md_green_500"
                        app:backgroundTint="@color/md_white_1000"
                        app:strokeColor="@color/md_green_500"
                        android:text="PASS"/>
    
                </LinearLayout>

                <EditText
                    android:id="@+id/mi_comments"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:inputType="text|textMultiLine"
                    android:hint="Comments"
                    android:focusable="false"
                    android:padding="8dp"
                    android:textSize="14sp"
                    android:background="@drawable/text_area_rounded"
                    android:gravity="top"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ttt_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
    
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Tilt Table Test"
                            android:layout_marginEnd="8dp"
                            android:textColor="@color/md_grey_800"
                            android:textSize="20sp" />

                        <info.androidhive.fontawesome.FontTextView
                            android:id="@+id/ttt_check"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fa_check_solid"
                            android:textColor="@color/md_green_500"
                            android:textSize="30sp"
                            app:solid_icon="true" />

                    </LinearLayout>

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/ttt_button"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/md_green_500"
                        app:rippleColor="@color/md_green_500"
                        app:backgroundTint="@color/md_white_1000"
                        app:strokeColor="@color/md_green_500"
                        android:text="PASS"/>
    
                </LinearLayout>

                <EditText
                    android:id="@+id/ttt_comments"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:inputType="text|textMultiLine"
                    android:hint="Comments"
                    android:focusable="false"
                    android:padding="8dp"
                    android:textSize="14sp"
                    android:background="@drawable/text_area_rounded"
                    android:gravity="top"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/nt_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
    
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Noise Test"
                            android:layout_marginEnd="8dp"
                            android:textColor="@color/md_grey_800"
                            android:textSize="20sp" />

                        <info.androidhive.fontawesome.FontTextView
                            android:id="@+id/nt_check"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fa_check_solid"
                            android:textColor="@color/md_green_500"
                            android:textSize="30sp"
                            app:solid_icon="true" />

                    </LinearLayout>

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/nt_button"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/md_green_500"
                        app:rippleColor="@color/md_green_500"
                        app:backgroundTint="@color/md_white_1000"
                        app:strokeColor="@color/md_green_500"
                        android:text="PASS"/>
    
                </LinearLayout>

                <EditText
                    android:id="@+id/nt_comments"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:inputType="text|textMultiLine"
                    android:hint="Comments"
                    android:focusable="false"
                    android:padding="8dp"
                    android:textSize="14sp"
                    android:background="@drawable/text_area_rounded"
                    android:gravity="top"/>
    
            </LinearLayout>

            <LinearLayout
                android:id="@+id/rt_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Rain Test"
                            android:layout_marginEnd="8dp"
                            android:textColor="@color/md_grey_800"
                            android:textSize="20sp" />

                        <info.androidhive.fontawesome.FontTextView
                            android:id="@+id/rt_check"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fa_check_solid"
                            android:textColor="@color/md_green_500"
                            android:textSize="30sp"
                            app:solid_icon="true" />

                    </LinearLayout>

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/rt_button"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/md_green_500"
                        app:rippleColor="@color/md_green_500"
                        app:backgroundTint="@color/md_white_1000"
                        app:strokeColor="@color/md_green_500"
                        android:text="PASS"/>
    
                </LinearLayout>

                <EditText
                    android:id="@+id/rt_comments"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:inputType="text|textMultiLine"
                    android:hint="Comments"
                    android:focusable="false"
                    android:padding="8dp"
                    android:textSize="14sp"
                    android:background="@drawable/text_area_rounded"
                    android:gravity="top"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/bt_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
    
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Brake Test"
                            android:layout_marginEnd="8dp"
                            android:textColor="@color/md_grey_800"
                            android:textSize="20sp" />

                        <info.androidhive.fontawesome.FontTextView
                            android:id="@+id/bt_check"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fa_check_solid"
                            android:textColor="@color/md_green_500"
                            android:textSize="30sp"
                            app:solid_icon="true" />

                    </LinearLayout>

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/bt_button"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/md_green_500"
                        app:rippleColor="@color/md_green_500"
                        app:backgroundTint="@color/md_white_1000"
                        app:strokeColor="@color/md_green_500"
                        android:text="PASS"/>
    
                </LinearLayout>

                <EditText
                    android:id="@+id/bt_comments"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:inputType="text|textMultiLine"
                    android:hint="Comments"
                    android:focusable="false"
                    android:padding="8dp"
                    android:textSize="14sp"
                    android:background="@drawable/text_area_rounded"
                    android:gravity="top"/>
                
            </LinearLayout>
            </LinearLayout>
        </ScrollView>
        
    </LinearLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>

